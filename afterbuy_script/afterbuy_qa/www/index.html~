	<script>
		var myScroll;
		var myScroll1;
		var myScroll2;
		var myScroll3;
		function loaded() {
			myScroll = new iScroll('wrapper', {
			
				momentum: false,
				hScrollbar: false, checkDOMChanges: true
			});
			myScroll1 = new iScroll('wrapper1', {
			
				momentum: false,
				hScrollbar: false, checkDOMChanges: true
			});
			myScroll2 = new iScroll('wrapper2', {
			
				momentum: false,
				hScrollbar: false, checkDOMChanges: true
			});
			
			myScroll3 = new iScroll('wrapper3', {
			
				momentum: false,
				hScrollbar: false, checkDOMChanges: true
			});
			enableFormsInIscroll();
		}
		document.addEventListener('touchmove', function (e) { e.preventDefault(); }, false);
		/* * * * * * * *
		 *
		 * Use this for high compatibility (iDevice + Android)
		 *
		 */
		document.addEventListener('DOMContentLoaded', function () { setTimeout(loaded, 200); }, false);
		$(window).load(function() {
			function onMobileInit() {
			console.log("mobile init");
			$.mobile.autoInitialize = false;
			}
			function onPageLoad() {// Document.Ready
				console.log("document ready");
				try {
				if(someCondition) {
				    document.location.hash = "#profile";
				} else {
				    document.location.hash = "#signIn";
				}
				} catch (exception) {
				} finally {
				$.mobile.initializePage();
				}
			}
		});
		$(function(){
			//if ($.browser.msie && $.browser.version == 10)
			if (navigator.appVersion.indexOf("MSIE") != -1)
			{
				$('#front-page .content h1').hide();
				$('#front-page .content h1').fadeIn(2000);
			};
		});
		$(document).bind('pageinit', function() {
			
		});
		$(window).load(function() {
			$(document).one("mobileinit", function () {
		        // Setting #container div as a jqm pageContainer
		        $.mobile.pageContainer = $('#container');
		        // Setting default page transition to slide
			    $.mobile.defaultPageTransition = 'slide';
	        });
		});
	
	//New Script Added
	function init_app() {
		// Set first page
		//panabee_set_active_page( '#image_preloader' );
		// Init pages
		$( 'div[data-role="page"]' ).trigger( 'pageinit' );
		// Init back buttons
		$( 'div[data-rel="back"]' ).on( 'tap', function() {
			panabee_go_back();
		});
		
		// Init links
		$( 'a' ).not( '[data-rel="back"]' ).on( 'touchstart', function() {
			var page_name = $( this ).attr( 'href' );
			if ( page_name.charAt(0) == '#' && $(page_name) ) {
				panabee_change_page( page_name );
			}
		});
					 								
		// Kill bounce effect browsers mobile browsers
		kill_browser_bounce();
		// Configure catalog page
		var sidebar_page = $( '#sidebar_page' );
		sidebar_page.height( win_height );
		sidebar_page.trigger( 'pageinit' );
		sidebar_page.show();
	}
		
	// Kills bounce effect on mobile browsers. Took so long to write!!!	
	function kill_browser_bounce() {
	    $( document ).on( 'touchstart', function( e ){
	        INITIAL_Y = e.originalEvent.touches[0].clientY;
	    });
	    $( document ).on( 'touchmove', function( e ) {
	        // Get scrollable ancestor if one exists
	        var scrollable_ancestor = $( e.target ).closest( '.content, .page_list' )[0];
	        
	        // Nothing scrollable? Block move.
	        if ( !scrollable_ancestor ) {
	        	e.preventDefault();
	        	return;
	        }
	        // If here, prevent move if at scrollable boundaries.
	        var scroll_delta = INITIAL_Y - e.originalEvent.touches[0].clientY;
	        var scroll_pos = scrollable_ancestor.scrollTop;	        
	        var at_bottom = (scroll_pos + $(scrollable_ancestor).height()) == scrollable_ancestor.scrollHeight;	        
	        if ( (scroll_delta < 0 && scroll_pos == 0) ||
	           	 (scroll_delta > 0 && at_bottom) ) {	        		
	        		e.preventDefault();
	        }
	    });
	}
	// ****************************************************************************************************
	// Catalog page
	// ****************************************************************************************************
	
	// Show catalog page
	function show_catalog() {		
		// Slide catalog page out
		$( '#sidebar_page' ).addClass( 'active' );
	}
	function hide_catalog() {
		$( '#sidebar_page' ).removeClass( 'active' );
	}
	//sidebar getting on click the dropdown icon
	$(document).bind('pageinit', function() {
		var widthdoc = $(document).width();
		
		//console.log(widthdoc);
		$( '#sidebar_page' ).css( 'left', widthdoc );
		$('.btn-dropdown').on('click', function() {
			$( '#sidebar_page' ).slideDown(50).toggleClass('active');
		});
		$('[data-role="page"]').swipe({
		  swipeLeft:function(event, direction, distance, duration, fingerCount) {
		    //console.log('wow you swaped ' + direction)
		    $( '#sidebar_page' ).slideDown(50).toggleClass('active');
		  },
		  swipeRight:function(event, direction, distance, duration, fingerCount) {
		    //console.log('wow you swaped ' + direction)
		    $( '#sidebar_page' ).removeClass('active');
		  },
		  threshold: 30
		});
		$('div.pagesContent').on('click', function() {
		  $( '#sidebar_page' ).removeClass('active');
		});
	});
	function formRedirection(e, event) {
		// $.mobile.changePage('#' + e);
		$.mobile.changePage( '#' + e, {transition: 'flip'});
		event.preventDefault();
	}
	$('html').addClass('js-en');
	// $('.sidebar-pos').hide();
	$(window).load(function() {
		var sidebar = $('div.sidebar-pos'),
			container = $('div#container');
		$('.btn-dropdown').bind('click', function() {
			
			sidebar.toggleClass('active');
			
			if( $('div.sidebar-pos:not(.active)') )  {
				hideSideBar();
			}
			if( sidebar.hasClass('active') ) {
				showSideBar();					
			} 
		});
		// Swipe left function for menu. If you want you can disable swipe left funtion delete code below.
		$('[data-role="page"]:not(#signin-page):not(#register-account)').on("swipeleft", function(){
			sidebar.toggleClass('active');
			
			showSideBar();
			
		});
		// Swipe left end
		// Swipe right function for menu. If you want you can disable swipe right funtion delete code below.
		$('body').on("swiperight", function(){
			hideSideBar();
		});
		// Swipe right end
	});
</script>
