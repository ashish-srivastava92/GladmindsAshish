<script>
	(function() {
		$('.popup-overlay').hide();
		$('.photo-access-link').bind('click', function() {
			var clickObj = $(this);
			var popup = $('.popup-notif'),
				overlay = $('.popup-overlay');
			popup.slideDown(200);
			overlay.fadeIn(400);
			$('.cancel-btn').bind('click', function() {
				overlay.fadeOut(400);
				popup.slideUp(300);
			});
		});
		
		/* adding new field after seleting "other" */
		
		$(document).ready(function() {
			var mainSelect = $('#ai-manu'),
				newSelect = $('#ai-manu option');
			//$(mainSelect).on('click', function() {
			$(mainSelect).change(function() {
				//debugger;
				var $this = $(this);
				if( $this.val() == 'Other' ) {
					$('<li class="js-created"><input data-role="none" type="text" id="ai-manu-next" name="ai-manu-next" placeholder="Add Manufacturer" class="ui-input-text ui-body-c ui-corner-all ui-shadow-inset"></li>').insertAfter(mainSelect.closest('li'));
				}
				else {
					$('li.js-created').remove();
				}
			});
		});
		
		/* Applying Bouncing to Notification icon */
		
		$(document).ready(function() {
			var container = $('.profile-apps');
			
				save = $('#ai-save');
			save.on('click', function() {
				var item_name = $('#ai-item').val(),
				// item_manu = $('#'),
				item_num = $('#ai-item-no').val(),
				pur_date = $('#ai-pur-date').val(),
				warranty = $('#ai-warranty').val(),
				insurance = $('#ai-insurance').val();
				console.log(item_name);
				console.log(item_num);
				console.log(pur_date);
				console.log(warranty);
				console.log(insurance);
			})		
		});
		
		var check_war_colr = function() { 
			var main_span = $('.check-war-ins p span'),
				main_span_color = main_span.data('check-value');
			
			if(main_span_color <= 10) {
				main_span.css('color', 'red');
			}
			else {
				main_span.css('color', 'green');
			}
		};
		check_war_colr();
		
		$(document).ready(function() {
			var FF = !(window.mozInnerScreenX == null);
			if(FF) {
				$('a').data('transition', 'fade');
			} 
		});
		
	})();
</script>
<script>
	// ****************************************************************************************************		
	// ****************************************************************************************************	
	// Page management
	// ****************************************************************************************************
	// ****************************************************************************************************		
	// CH: Reenable buttons that were disabled to prevent double taps. Hack???
	function panabee_go_back() {
		var cur_page = PAGE_HISTORY.pop();
		var last_page = PAGE_HISTORY[ PAGE_HISTORY.length-1 ];
		if ( last_page ) {
			$( cur_page ).find( '.disabled' ).removeClass( 'disabled' );
			$( cur_page ).hide();
			$( last_page ).show().trigger( 'pagebeforeshow' ).trigger( 'pageshow' );		
		}
	}
</script>
